<h1 class="text-center mb-5">Tambah Kontak</h1>

<div class="col-lg-8 mx-auto">
    <form action="/contact" method="POST">
        <div class="form-floating mb-3">
            <input name="name" type="text" class="form-control" id="floatingName" placeholder="Jhon Doe">
            <label for="floatingName">Nama</label>
        </div>
        <div class="form-floating mb-3">
            <input name="noTelp" type="number" class="form-control" id="floatingNoTelp" placeholder="081290789909">
            <label for="floatingNoTelp">Nomor Telepon</label>
        </div>
        <div class="d-flex justify-content-end">
            <button class="btn btn-primary" type="submit">Simpan</button>
        </div>
    </form>
</div>